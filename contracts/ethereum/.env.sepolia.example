# Sepolia Deployment Configuration

# RPC Endpoints
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
# Or use Alchemy: https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Deployer Wallet (needs Sepolia ETH)
PRIVATE_KEY=your_deployer_private_key_here

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key

# Contract Configuration
INITIAL_OWNER=0xYourMultisigOrOwnerAddress
MIN_SIGNATURES=3
TOTAL_ATTESTERS=5

# Attester Addresses (must match Substrate attesters)
ATTESTER_1=0xAttester1Address
ATTESTER_2=0xAttester2Address
ATTESTER_3=0xAttester3Address
ATTESTER_4=0xAttester4Address
ATTESTER_5=0xAttester5Address

# Gas Settings
REPORT_GAS=false
