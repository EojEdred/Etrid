{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GovernanceProposal",
  "type": "object",
  "properties": {
    "proposalId": { "type": "integer" },
    "title": { "type": "string" },
    "description": { "type": "string" },
    "category": { "type": "string" },
    "stakeRequired": { "type": "number" },
    "submissionDate": { "type": "string", "format": "date-time" }
  },
  "required": ["proposalId", "title", "description", "stakeRequired"]
}