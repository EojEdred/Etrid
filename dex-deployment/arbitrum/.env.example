# Arbitrum Deployment Configuration
# Copy this file to .env and fill in your values
# CRITICAL: Keep your private key secret! Never commit .env to git!

PRIVATE_KEY=your_private_key_here

# RPC Endpoints (optional - defaults provided)
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
ARBITRUM_TESTNET_RPC=https://sepolia-rollup.arbitrum.io/rpc

# Arbiscan API Key (for contract verification)
# Get one free at: https://arbiscan.io/myapikey
ARBISCAN_API_KEY=your_arbiscan_api_key_here

# Foundation Multisig Address (transfer ownership after deployment)
FOUNDATION_MULTISIG=your_multisig_address_here

# Bridge Contract Addresses (set after bridge deployments)
ARBITRUM_BRIDGE=your_arbitrum_bridge_address_here
CROSS_CHAIN_BRIDGE=your_cross_chain_bridge_address_here
